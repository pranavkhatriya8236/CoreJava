package in.co.rays.Hello;

public class TestLargestNuumber {
public static void main(String[] args) {
	int[] arr = {10,20,40,50,110} ;
	 int a= 0;
	int b=0;
	 
	 
	 for(int i=0;i <arr.length;i++)  {
	 if (arr[i]>b) {
		 b=arr[i];
		 
		if(arr[i]>b && arr[i]<a) {
			
				a=arr[i];
			}
			
		 }
	 
	System.out.println(b);
	
	 }
	
	
	
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
	
	
	
	
	
	
	
	
	
	
	
}
