package in.co.rays.Hello;

public class PrimeNumber2 {
public static void main(String[] args) {
	
int number= 12;
int count = 0;
 
for(int i =0; i<=12;i++) {
	

if (number% i==0) {
count++;
}
}
if(count==0)	{
	System.out.println("not prime number");
	
}else {
	System.out.println("prime number");
}
	
	
	
}
}
